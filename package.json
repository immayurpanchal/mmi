{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mmi",
  "title": "MMI",
  "description": "Market Mood Indicator shows the current market mood from ticker tape",
  "icon": "command-icon.png",
  "author": "immayurpanchal",
  "categories": [
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Market Mood Indicator",
      "description": "Template for showing a menu bar extra",
      "mode": "menu-bar",
      "interval": "1h"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.1",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
